
/*
class Node{
    int data;
    Node next;
    
    Node(int x){
        data = x;
        next = null;
    }
}
*/
class Solution
{
    //Function to insert a node at the beginning of the linked list.
    Node insertAtBeginning(Node head, int x)
    {
        Node temp=new Node(x);
        // code here
        temp.next=head;
        head=temp;
        
        return head;
    }
    
    //Function to insert a node at the end of the linked list.
    Node insertAtEnd(Node head, int x)
    {
        // code here
                Node temp=new Node(x);

        if(head==null)
        {
        return temp;
        }
        
            Node cur=head;
            while(cur.next!=null)
            {
                cur=cur.next;
            }
            cur.next=temp;
        
        return head;
    }
}
